{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "dist/functions"
  },
  "hosting": [
    {
      "public": "dist/public",
      "rewrites": [
        {
          "source": "/book/**",
          "function": "book"
        },
        {
          "source": "/books",
          "function": "books"
        },
        {
          "source": "**/**",
          "function": "index"
        }
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  }
}
